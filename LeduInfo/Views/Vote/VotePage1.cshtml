@model IEnumerable<LeduInfo.Models.ImgPath>


@{
    ViewBag.Title = "VotePage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("../../Content/animation.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("../../Content/imgResource.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("../../Scripts/jquery-1.8.2.js")" type="text/javascript"></script>
<link href="@Url.Content("../../Content/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("../../Scripts/VotePanel.js")" type="text/javascript">
    <h2>VotePage</h2>


</script>

@{
    var quantity = 2;
}


<ul class="thumbnails">
    @for (int i = 0; i < quantity; i++)
    {
        <li class="span4">
            <a href="#" class="thumbnail">
                <div id="item-receiver-@i"></div>
            </a>
        </li>
    }

</ul>


<div id="thumbnail-wrapper">
    <img src="../../Images/plus.png" width="60" id="plus">

    <ul class="item-list">
        @foreach (var item in Model)
        {

            <li>
                <img class="item" id="@item.pathID" src="/House/GetImageList/@item.pathID" width="70" alt=""></li> 
        }
    </ul>
